{
  "name": "Oink",
  "version": "0.0.0",
  "description": "Behavioral Finance",
  "scripts": {
    "start": "NODE_ENV=production node --harmony server/index.js",
    "dev": "NODE_ENV=dev node --harmony server/index.js",
    "copyEnv": "cp ./server/env/envConfig-temp.js ./server/env/envConfig.js",
    "build": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "clean": "rm -r ./public/bundle.min.js",
    "test": "NODE_ENV=test mocha --timeout 10000 --compilers js:babel/register test/loadtests.js",
    "testdev": "NODE_ENV=dev mocha --timeout 10000 --compilers js:babel/register test/loadtests.js",
    "lint": "jscs ./"
  },
