{
  "excludeFiles": ["index.js", "webpack.config.js", "webpack.production.config.js", "node_modules/**", "public/**", "coverage/**", "test/**"],
  "preset": "airbnb",
  "esprima": "esprima-fb",
  "maxErrors": null,
  "disallowSemicolons": true,
  "requireSemicolons": null,
  "requireTrailingComma": null,
  "requireCamelCaseOrUpperCaseIdentifiers": null,
  "requireArrowFunctions": true,
  "requireParenthesesAroundArrowParam": true,
  "requireLineFeedAtFileEnd": null,
  "disallowTrailingWhitespace": "ignoreEmptyLines",
  "disallowMultipleLineStrings": false
}
